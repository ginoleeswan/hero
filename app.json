{
  "expo": {
    "name": "hero",
    "slug": "hero",
    "owner": "gl-studio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/hero-icon.png",
    "splash": {
      "image": "./app/assets/hero-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C3C44"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "./app/assets/images/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ginolee.hero"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/hero-icon-android.png",
        "backgroundColor": "#293C43"
      }
    },
    "web": {
      "favicon": "./app/assets/hero-favicon.png"
    },
    "description": "A superhero encyclopaedia made in React Native",
    "githubUrl": "https://github.com/ginoleeswan/hero"
  }
}
